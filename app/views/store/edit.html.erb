<%= form_for @store, url: {action: :update} do |f| %>
  <%= f.label :name, 'name' %> 
  <%= f.text_field :name %> 
  <%= f.label :address, 'address' %> 
  <%= f.password_field :address %> 
  <%= f.label :category_1, 'category_1' %> 
  <%= f.text_field :category_1 %>
  <%= f.label :category_2, 'category_2' %> 
  <%= f.text_field :category_2 %>
  <%= f.submit '登録' %>
<% end %>